{
  "version": 2,
  "projects": {
    "angular-implementation": {
      "projectType": "application",
      "root": "apps/angular-implementation",
      "sourceRoot": "apps/angular-implementation/src",
      "prefix": "evercraft",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/angular-implementation",
            "index": "apps/angular-implementation/src/index.html",
            "main": "apps/angular-implementation/src/main.ts",
            "polyfills": "apps/angular-implementation/src/polyfills.ts",
            "tsConfig": "apps/angular-implementation/tsconfig.app.json",
            "assets": [
              "apps/angular-implementation/src/favicon.ico",
              "apps/angular-implementation/src/assets"
            ],
            "styles": ["apps/angular-implementation/src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/angular-implementation/src/environments/environment.ts",
                  "with": "apps/angular-implementation/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "angular-implementation:build:production"
            },
            "development": {
              "browserTarget": "angular-implementation:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "angular-implementation:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/angular-implementation/src/**/*.ts",
              "apps/angular-implementation/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/angular-implementation"],
          "options": {
            "jestConfig": "apps/angular-implementation/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "how-to-jest": {
      "root": "libs/how-to-jest",
      "sourceRoot": "libs/how-to-jest/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/how-to-jest/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/how-to-jest"],
          "options": {
            "jestConfig": "libs/how-to-jest/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "functional-ts-ability-scores": {
      "root": "libs/ts-functional/ability-scores",
      "sourceRoot": "libs/ts-functional/ability-scores/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/ts-functional/ability-scores/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ts-functional/ability-scores"],
          "options": {
            "jestConfig": "libs/ts-functional/ability-scores/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "none"
    },
    "@nrwl/angular:library": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "css"
    }
  },
  "defaultProject": "angular-implementation"
}
